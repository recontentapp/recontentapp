{
  "name": "figma-plugin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit -p plugin-src && tsc --noEmit -p ui-src",
    "build:plugin": "esbuild plugin-src/index.ts --bundle --outfile=dist/index.js",
    "build:ui": "npx vite build --minify esbuild --emptyOutDir=false",
    "prebuild": "rm -rf ./dist && mkdir ./dist && cp -fr ./manifest.json ./dist/manifest.json",
    "build": "yarn build:ui -m production && yarn build:plugin --minify",
    "start": "cp -fr ./manifest.json ./dist/manifest.json && concurrently -n plugin,ui \"yarn build:plugin --watch\" \"yarn build:ui -m development --watch\""
  },
  "dependencies": {
    "@stitches/react": "1.2.8",
    "diff": "5.1.0",
    "figma-ui-kit": "0.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-query": "3.34.15"
  },
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "@types/diff": "^5.0.2",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "concurrently": "6.3.0",
    "esbuild": "0.19.2",
    "prettier": "2.3.2",
    "typescript": "4.5.5",
    "vite": "2.5.2",
    "vite-plugin-singlefile": "0.5.1",
    "vite-svg-loader": "3.3.0"
  }
}
