{
  "name": "@recontentapp/file-formats",
  "version": "0.4.3",
  "license": "AGPL-3.0-only",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "file format",
    "i18n"
  ],
  "homepage": "https://github.com/recontentapp/recontentapp",
  "repository": {
    "type": "git",
    "url": "https://github.com/recontentapp/recontentapp"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:src": "esbuild src/index.ts --bundle --platform=node --packages=external --sourcemap --analyze --outfile=dist/index.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir ./dist",
    "build": "rm -rf dist && yarn build:src && yarn build:types",
    "test": "jest"
  },
  "dependencies": {
    "exceljs": "4.4.0",
    "fast-xml-parser": "4.3.6",
    "fs-extra": "11.2.0",
    "papaparse": "5.4.1",
    "php-parser": "3.1.5",
    "yaml": "2.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "esbuild": "0.20.2",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3"
  }
}
