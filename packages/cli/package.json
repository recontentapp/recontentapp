  },
  "dependencies": {
    "@recontentapp/file-formats": "0.4.2",
    "ascii-table3": "0.9.0",
    "commander": "12.0.0",
    "fs-extra": "11.2.0",
    "query-string": "7.1.3",
    "yaml": "2.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.12.7",
    "esbuild": "0.20.2",
    "typescript": "5.4.3"
  }
}
