{
  "name": "@recontentapp/cli",
  "version": "0.4.2",
  "license": "AGPL-3.0-only",
  "private": false,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --packages=external --sourcemap --analyze --outfile=dist/index.js",
